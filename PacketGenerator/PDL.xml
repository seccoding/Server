<?xml version="1.0" encoding="utf-8" ?> 
<!-- 
(PDL)Packet Definition List
usage="Client" : Client -> Server (서버가 처리)
usage="Server" : Server -> Client (클라이언트가 처리)
-->
<PDL>
  <packet name="ClientChat" usage="Client">
    <string name="chat" />
    <class name="Test">
      <string name="testName" />
    </class>
  </packet>
  <packet name="ServerChat" usage="Server">
    <int name="playerId" />
    <string name="chat"/>
  </packet>
</PDL>