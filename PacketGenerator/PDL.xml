<?xml version="1.0" encoding="utf-8" ?> 
<!-- 
(PDL)Packet Definition List
PacketName 에 C_가 붙으면 Client -> Server (서버가 처리)
              S_가 붙으면 Server -> Client (클라이언트가 처리)
-->
<PDL>
  <packet name="ClientChat" usage="Client">
    <string name="chat" />
  </packet>
  <packet name="ServerChat" usage="Server">
    <int name="playerId" />
    <string name="chat"/>
  </packet>
</PDL>